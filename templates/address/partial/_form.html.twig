{{ form_start(addressForm) }}
<div class="row">
    <div class="col-9">
        {{ form_row(addressForm.nameOrNumber) }}
    </div>
    <div class="col-3">
        {{ form_row(addressForm.floor) }}
    </div>
    <div class="col-12">
        {{ form_row(addressForm.postcode) }}
    </div>
    <div class="col-6">
        {{ form_row(addressForm.city) }}
    </div>
    <div class="col-6">
        {{ form_row(addressForm.county) }}
    </div>
    <div class="col-12">
        {{ form_row(addressForm.lineOne) }}
        {{ form_row(addressForm.lineTwo) }}
        {{ form_row(addressForm.country) }}
    </div>
</div>
<div class="text-end">
    <button type="submit" class="btn btn-outline-primary">{{ 'save'|trans }}</button>
</div>
{{ form_end(addressForm) }}
